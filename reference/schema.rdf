    <!-- Record Service Plugin -->
    <owl:Class rdf:about="dsn:data.vios.network/o/RecordServicePlugin">
        <rdfs:label>Record Service Plugin</rdfs:label>
        <rdfs:comment>A Record plugin link to a third-party service.</rdfs:comment>
    </owl:Class>
    <owl:DataProperty rdf:about="dsn:data.vios.network/o/Plugin/classes">
        <rdfs:comment>A string delimited list of Bootstrap image classes of the Record plugin, e.g. "fa fa-apple"</rdfs:comment>
        <owl:domain rdf:resource="dsn:data.vios.network/o/RecordServicePlugin"/>
        <owl:domain rdf:resource="dsn:data.vios.network/o/RecordOptionPlugin"/> 
    </owl:DataProperty>
    <owl:DataProperty rdf:about="dsn:data.vios.network/o/Plugin/actionUrlTemplate">
        <rdfs:comment>URL to place in the "href" attribute of the plugin anchor tag. Supports the following placeholder tags: e.g. ${http://www.example.com/onto/MyProperty, ...} = the value of http://www.example.com/onto/MyProperty or ... in the matched record, the value of the first non-empty property in the list is used to fill the placeholder; ${keywords} = the current data canvas keywords</rdfs:comment>
        <owl:domain rdf:resource="dsn:data.vios.network/o/RecordServicePlugin"/>
        <owl:domain rdf:resource="dsn:data.vios.network/o/RecordTabPlugin"/>
        <owl:domain rdf:resource="dsn:data.vios.network/o/RecordViewPlugin"/>
        <owl:domain rdf:resource="dsn:data.vios.network/o/RecordDescriptionPlugin"/>
    </owl:DataProperty>
    <owl:DataProperty rdf:about="dsn:data.vios.network/o/RecordServicePlugin/tooltip">
        <rdfs:comment>Specifies a short description of the action associated with the plugin, usually begins with a verb, .e.g. "View in iTunes"</rdfs:comment>
        <owl:domain rdf:resource="dsn:data.vios.network/o/RecordServicePlugin"/>
        <owl:domain rdf:resource="dsn:data.vios.network/o/RecordOptionPlugin"/> 
    </owl:DataProperty>


    <owl:Class rdf:about="dsn:data.vios.network/o/RecordOptionPlugin">
        <rdfs:label>Record Option Plugin</rdfs:label>
        <rdfs:comment>A Record plugin which adds a button to the Options tab in Record Viewer.</rdfs:comment>
        <rdfs:comment>When specified, the vo:classes value is added to the &lt;i&gt; child of the &lt;button&gt; element.</rdfs:comment>
    </owl:Class>
    <owl:DataProperty rdf:about="dsn:data.vios.network/o/RecordOptionPlugin/labelPrefix">
        <rdfs:comment>Specifies a label prefix for the Option button.</rdfs:comment>
        <owl:domain rdf:resource="dsn:data.vios.network/o/RecordOptionPlugin"/>
    </owl:DataProperty>
    <owl:ObjectProperty rdf:about="dsn:data.vios.network/o/RecordOptionPlugin/labelSuffix">
        <rdfs:comment>The value of the property in the matched Record is used as the suffix for the Option button label.</rdfs:comment>
        <owl:domain rdf:resource="dsn:data.vios.network/o/RecordOptionPlugin"/>
        <owl:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    </owl:ObjectProperty>

    <owl:Class rdf:about="dsn:data.vios.network/o/RecordLegendPlugin">
        <rdfs:label>Record Legend Plugin</rdfs:label>
        <rdfs:comment>A Record plugin which adds a Legend section in Record Viewer.</rdfs:comment>
    </owl:Class>
    <owl:DataProperty rdf:about="dsn:data.vios.network/o/Plugin/label">
        <rdfs:label>Specifies a label for the component.</rdfs:label>
        <owl:domain rdf:resource="dsn:data.vios.network/o/RecordLegendPlugin"/>
        <owl:domain rdf:resource="dsn:data.vios.network/o/RecordTabPlugin"/>
        <owl:domain rdf:resource="dsn:data.vios.network/o/RecordViewPlugin"/>
    </owl:DataProperty>

    <owl:DataProperty rdf:about="dsn:data.vios.network/o/Plugin/componentSparqlTemplate">
        <rdfs:comment>Specifies a SPARQL query used to populate the component. Each result set item is added to the Record Viewer using the $.append() jQuery method or equivalent. The SPARQL is executed against the current dataspace. The query projection must contain two variables: an IRI or Literal as the first, and a label as the second.</rdfs:comment>
        <owl:domain rdf:resource="dsn:data.vios.network/o/RecordLegendPlugin"/>
        <owl:domain rdf:resource="dsn:data.vios.network/o/RecordTabPlugin"/>
        <owl:domain rdf:resource="dsn:data.vios.network/o/RecordViewPlugin"/>
    </owl:DataProperty>

    <owl:Class rdf:about="dsn:data.vios.network/o/RecordDescriptionPlugin">
        <rdfs:label>Record Description Plugin</rdfs:label>
